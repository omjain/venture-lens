# Server Configuration
PORT=3001
NODE_ENV=development

# Google Vertex AI Configuration (Preferred)
# For Vertex AI: Set PROJECT_ID and LOCATION, then run: gcloud auth application-default login
GEMINI_PROJECT_ID=your_project_id_here
GEMINI_LOCATION=us-central1

# OR use Gemini API (Google AI Studio) - Alternative
# GEMINI_API_KEY=your_gemini_api_key_here

# CORS Configuration
FRONTEND_URL=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
